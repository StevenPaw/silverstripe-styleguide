{
    "name": "stevenpaw/silverstripe-styleguide",
    "description": "A style guide for silverstripe with options for elemental",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "styleguide",
        "placeholder",
        "elemental"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^6.0",
        "dnadesign/silverstripe-elemental": "^6.1"
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
